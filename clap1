#Program to take input from user and then print the same using switch case(nested)
#include <stdio.h>
int main() 
{
    int ID = 500;
    int password = 365;
    printf("Plese Enter Your ID:\n ");
    scanf("%d", & ID);
    switch (ID) 
    {
        case 500:
            printf("Enter your password:\n ");
            scanf("%d", & password);
            switch (password) 
            {
                case 365:
                    printf("Welcome Dear Programmer.\n");
                    break;
                default:
                    printf("Incorrect Password.");
                    break;
             }
            break;
         default:
             printf("Incorrect ID");
             break;
      }
}
